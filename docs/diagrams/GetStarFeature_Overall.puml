@startuml
title Overall GetStar Feature Flow

' --- High resolution for clear PNG output ---
skinparam dpi 300
skinparam sequenceReferenceBackgroundColor #7cb9e8

participant "<<class>>\n QuizMos" as quizmos
participant "<<class>>\n Parser" as parser
participant "c:GetStarCommand" as getStarCommand
participant "flashcards : FlashcardList" as flashcardList
participant "<<class>>\n Ui" as ui
participant "storage : Storage" as storage

quizmos -> parser : parseCommand(command : String)
activate parser

parser -> getStarCommand : <<create>> GetStarCommand()
activate getStarCommand

getStarCommand --> parser : return GetStarCommand
deactivate getStarCommand

parser --> quizmos : return GetStarCommand
deactivate parser

quizmos -> getStarCommand : execute(flashcards : FlashcardList, storage : Storage)
activate getStarCommand

getStarCommand -> flashcardList : getStarredFlashcardsString()
activate flashcardList
flashcardList --> getStarCommand : starredFlashcardsList : String
deactivate flashcardList

getStarCommand -> ui : showStarredFlashcardsList(starredFlashcardsList : String)

getStarCommand --> quizmos
deactivate getStarCommand
destroy getStarCommand

@enduml
